# React App Environment Variables Template
# Copy this file to .env and update the values as needed
# Note: All variables must start with REACT_APP_ to be accessible in React

# Application Configuration
REACT_APP_NAME=React MUI Sample App
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=A sample React application with Material-UI components

# API Configuration
REACT_APP_API_BASE_URL=https://jsonplaceholder.typicode.com
REACT_APP_API_TIMEOUT=5000
REACT_APP_USERS_API_URL=https://jsonplaceholder.typicode.com/users

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG_MODE=false
REACT_APP_SHOW_CONTACT_STATS=true

# Theme Configuration
REACT_APP_PRIMARY_COLOR=#1976d2
REACT_APP_SECONDARY_COLOR=#dc004e
REACT_APP_THEME_MODE=light

# Contact Management Settings
REACT_APP_MAX_CONTACTS=100
REACT_APP_DEFAULT_COUNTRY_CODE=+1

# API Configuration (Replace with your actual values)
REACT_APP_API_BASE_URL=https://api.example.com
REACT_APP_API_KEY=your-api-key-here
REACT_APP_API_TIMEOUT=5000

# Database Configuration (Backend only - NOT for React frontend)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=myapp
# DB_USER=dbuser
# DB_PASSWORD=secure-password-here

# Authentication (Backend only - NOT for React frontend)
# JWT_SECRET=your-super-secret-jwt-key
# SESSION_SECRET=your-session-secret

# External Services (Replace with your actual keys)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_SENTRY_DSN=https://example@sentry.io/project
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_xxxxxxxxx
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-key

# Feature Flags for Security
REACT_APP_ENABLE_LOGGING=false
REACT_APP_DEBUG_MODE=false
REACT_APP_MOCK_API=false
